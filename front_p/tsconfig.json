  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts", "components/addProperty/AddProperty.jsx", "components/login/login.jsx", "app/dashboard/page.jsx", "app/dashboard/page.jsx", "components/VerifyEmail/VerifyEmail.js", "components/getStartedd/step4.jsx", "app/create-site/add-property/page.tsx", "app/create-site/signup/step3.jsx", "app/create-site/email-verified/page.jsx", "app/create-site/verify-email/page.jsx", "app/scraping/page.jsx"],
  "exclude": ["node_modules"]
}
